{
  "kind": "collectionType",
  "collectionName": "facturas",
  "info": {
    "singularName": "factura",
    "pluralName": "facturas",
    "displayName": "Facturas"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "noFactura": {
      "type": "biginteger",
      "required": false
    },
    "fechaLimite": {
      "type": "date",
      "required": false
    },
    "cai": {
      "type": "string",
      "required": false
    },
    "rtnCliente": {
      "type": "string",
      "default": "00000000000000",
      "minLength": 14,
      "maxLength": 14
    },
    "nombreCliente": {
      "type": "string",
      "default": "CF"
    },
    "codigoNumFactura": {
      "type": "string",
      "required": false
    },
    "subtotal": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "totalImpuestoQ": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "totalImpuestoD": {
      "type": "decimal"
    },
    "totalDescuento": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "totalExento": {
      "type": "decimal"
    },
    "totalExonerado": {
      "type": "decimal"
    },
    "estado": {
      "type": "enumeration",
      "default": "PAGADO",
      "enum": [
        "PAGADO",
        "PENDIENTE",
        "CANCELADO"
      ]
    },
    "infoCancelacion": {
      "type": "text"
    },
    "adjunto": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "noCompraExenta": {
      "type": "string"
    },
    "noConstRegExonerado": {
      "type": "string"
    },
    "noSAG": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "empresa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::empresa.empresa"
    },
    "sucursal": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sucursal.sucursal"
    }
  }
}
